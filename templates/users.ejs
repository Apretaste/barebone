<div class="row">
	<div class="col s12">
		<ul class="tabs tabs-fixed-width">
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'BAREBONE RESULTS'})">Resultados</a></li>
			<li class="tab"><a href="#" class="active">Usuarios</a></li>
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'BAREBONE RULES'})">Reglas</a></li>
		</ul>
	</div>
</div>

<div class="row">
	<% users.forEach(function(item){ %>
		<div class="col s4 m2">
			<div class="card center-align">
				<div class="content">
					<div class="person-avatar circle" face="<%= item.avatar %>" color="<%= item.avatarColor %>" size="40" online="1"></div>
					@<%= item.username %>
				</div>
				<div class="action grey lighten-2 green-text">
					<a href="#!"><i class="material-icons" onclick="apretaste.send({command: 'BAREBONE LIST'})">chat</i></a>
					<a href="#!"><i class="material-icons" onclick="apretaste.send({command: 'BAREBONE LIST'})">person</i></a>
				</div>
			</div>
		</div>
	<% }) %>
</div>

<!-- PAGINATION -->
<% if(pages > 1) { %>
	<div class="row">
		<div class="col s12 center-align">
			<ul class="pagination">
				<li class="waves-effect"><a href="#!"><i class="material-icons">chevron_left</i></a></li>
				<li class="waves-effect"><%= page %> / <%= pages %></li>
				<li class="waves-effect"><a href="#!"><i class="material-icons">chevron_right</i></a></li>
			</ul>
		</div>
	</div>
<% } %>

<div class="bottom-space"></div>

<!-- SUBMIT -->
<div class="fixed-action-btn">
	<a class="btn-floating btn-large waves-effect waves-light green" href="#!" onclick="apretaste.send({'command':'BAREBONE SEARCH'})">
		<i class="material-icons">search</i>
	</a>
</div>
